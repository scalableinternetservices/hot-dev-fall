<?xml version="1.0"?><tsung loglevel="notice">

  <clients>
    <client host="localhost" use_controller_vm="true" maxusers="15000"/>
  </clients>

  <servers>
    <server host="sscaching.enfgiuckxv.us-west-2.elasticbeanstalk.com" port="80" type="tcp"/>
  </servers>


  <load>
    <arrivalphase phase="1" duration="30" unit="second">
      <users arrivalrate="1" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="2" duration="30" unit="second">
      <users arrivalrate="2" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="3" duration="30" unit="second">
      <users arrivalrate="4" unit="second"/>
    </arrivalphase>
  </load>

  <options>
    <option name="global_ack_timeout" value="2000"/>

    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>

  <sessions>
        <session name="http-example" probability="90" type="ts_http">

      <request>
        <http url="/" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <setdynvars sourcetype="random_string" length="30">
        <var name="rdm_signup_email_name"/>
      </setdynvars>

    <setdynvars sourcetype="random_string" length="30">
        <var name="rdm_signup_email_domain"/>
      </setdynvars>

    <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/signup" version="1.1" method="GET">
          <http_header name="Cookie" value="%%_cookie%%"/>
        </http>
      </request>

      <setdynvars sourcetype="eval" code="fun({Pid,DynVars})->                 {ok,Val}=ts_dynvars:lookup(authenticity_token,DynVars),                 http_uri:encode(Val) end.">
            <var name="encoded_authenticity_token"/>
      </setdynvars>

    <thinktime value="2" random="true"/>

        <request subst="true">
            <http url="/users" version="1.1" contents="utf8=%E2%9C%93&amp;authenticity_token=%%_encoded_authenticity_token%%&amp;user%5Bemail%5D=%%_rdm_signup_email_name%%%40%%_rdm_signup_email_domain%%.com&amp;user%5Bpassword%5D=testing&amp;user%5Bpassword_confirmation%5D=testing&amp;commit=Sign+up" content_type="application/x-www-form-urlencoded" method="POST">
            </http>
        </request>

        <request>
        <http url="/static_pages/usertype" version="1.1" method="GET"/>
      </request>

    <thinktime value="2" random="true"/>

      <request>
      <dyn_variable name="authenticity_token" xpath="/html/head/meta[@name='csrf-token']/@content"/>
        <http url="/joiner" version="1.1" method="GET"/>
      </request>

      <setdynvars sourcetype="eval" code="fun({Pid,DynVars})->                 {ok,[Val]}=ts_dynvars:lookup(authenticity_token,DynVars),                 http_uri:encode(Val) end.">
            <var name="encoded_authenticity_token"/>
      </setdynvars>

      <request subst="true">
            <http url="/joiners" version="1.1" contents="utf8=%E2%9C%93&amp;authenticity_token=%%_encoded_authenticity_token%%&amp;joiner%5Bservice%5D=Netflix&amp;joiner%5Buser_id%5D=-1&amp;joiner%5Bstatus%5D=Pending&amp;commit=Join+A+Group" content_type="application/x-www-form-urlencoded" method="POST">
            </http>
        </request>

    </session>
        <session name="http-example" probability="10" type="ts_http">

      <request>
        <http url="/" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <setdynvars sourcetype="random_string" length="30">
        <var name="rdm_signup_email_name"/>
      </setdynvars>

    <setdynvars sourcetype="random_string" length="30">
        <var name="rdm_signup_email_domain"/>
      </setdynvars>

    <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/signup" version="1.1" method="GET">
          <http_header name="Cookie" value="%%_cookie%%"/>
        </http>
      </request>

      <setdynvars sourcetype="eval" code="fun({Pid,DynVars})->                 {ok,Val}=ts_dynvars:lookup(authenticity_token,DynVars),                 http_uri:encode(Val) end.">
            <var name="encoded_authenticity_token"/>
      </setdynvars>

    <thinktime value="2" random="true"/>

        <request subst="true">
            <http url="/users" version="1.1" contents="utf8=%E2%9C%93&amp;authenticity_token=%%_encoded_authenticity_token%%&amp;user%5Bemail%5D=%%_rdm_signup_email_name%%%40%%_rdm_signup_email_domain%%.com&amp;user%5Bpassword%5D=testing&amp;user%5Bpassword_confirmation%5D=testing&amp;commit=Sign+up" content_type="application/x-www-form-urlencoded" method="POST">
            </http>
        </request>

        <request>
        <http url="/static_pages/usertype" version="1.1" method="GET"/>
      </request>

    <thinktime value="2" random="true"/>

      <request>
      <dyn_variable name="authenticity_token" xpath="/html/head/meta[@name='csrf-token']/@content"/>
        <http url="/sharer" version="1.1" method="GET"/>
      </request>

      <setdynvars sourcetype="eval" code="fun({Pid,DynVars})->                 {ok,[Val]}=ts_dynvars:lookup(authenticity_token,DynVars),                 http_uri:encode(Val) end.">
            <var name="encoded_authenticity_token"/>
      </setdynvars>

      <request subst="true">
            <http url="/sharers" version="1.1" contents="utf8=%E2%9C%93&amp;authenticity_token=%%_encoded_authenticity_token%%&amp;sharer%5Bservice%5D=Netflix&amp;sharer%5Bsize%5D=Standard&amp;sharer%5Baccount_id%5D=test@gmail.com&amp;sharer%5Baccount_password%5D=password&amp;sharer%5Buser_id%5D=0&amp;sharer%5Bstatus%5D=Pending&amp;sharer%5Bplan_cost%5D=-1&amp;commit=Share" content_type="application/x-www-form-urlencoded" method="POST">
            </http>
        </request>

    </session>
  </sessions>
</tsung>
